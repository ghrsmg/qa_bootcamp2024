<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression" verbose="1" >
    <listeners>
        <listener class-name="customReporting.CustomTestReporter"/>
        <listener class-name="customReporting.CustomTestListener"/>
    </listeners>
    <test name="JuiceShop - MainPage Test" >

        <classes>
            <class name="juiceShop.tests.MainTest" />
        </classes>
    </test>
    <test name="JuiceShop - LoginPage Test" >

        <classes>
            <class name="juiceShop.tests.LoginTest" />
        </classes>
    </test>
    <test name="JuiceShop - RegisterPage Test" >

        <classes>
            <class name="juiceShop.tests.RegisterTest" />
        </classes>
    </test>
    <test name="JuiceShop - Cookie Test" >

        <classes>
            <class name="juiceShop.tests.CookieTest" />
        </classes>
    </test>
    <test name="PetStore - Pet Tests" >

        <classes>
            <class name="petStore.PetTest" />
        </classes>
    </test>
    <test name="PetStore - User Tests" >

        <classes>
            <class name="petStore.UserTest" />
        </classes>
    </test>

</suite>